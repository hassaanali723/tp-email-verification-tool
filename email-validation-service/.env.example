# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=Email Validation Service

# Validation Settings
DNS_TIMEOUT=10
SMTP_TIMEOUT=30
MAX_CONCURRENT_VALIDATIONS=5

# Cache Settings
CATCH_ALL_CACHE_TTL=3600

# Cache TTL Settings (in seconds)
CACHE_TTL_FULL_RESULT=86400
CACHE_TTL_MX_RECORDS=172800
CACHE_TTL_BLACKLIST=21600
CACHE_TTL_DISPOSABLE=604800
CACHE_TTL_CATCH_ALL=86400

# Cache Keys Prefix
CACHE_KEY_PREFIX=email_validation:

# Cache Control
ENABLE_RESULT_CACHE=True
ENABLE_MX_CACHE=True
ENABLE_BLACKLIST_CACHE=True
ENABLE_DISPOSABLE_CACHE=True
ENABLE_CATCH_ALL_CACHE=True

# RabbitMQ settings
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=your_rabbitmq_user
RABBITMQ_PASS=your_rabbitmq_password
RABBITMQ_VHOST=/
RABBITMQ_QUEUE=email_validation
RABBITMQ_DLQ=email_validation_dlq

# Redis settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=your_redis_password
REDIS_RESULT_EXPIRY=3600

# Worker settings
WORKER_BATCH_SIZE=5
WORKER_PREFETCH_COUNT=1
MAX_RETRIES=3 